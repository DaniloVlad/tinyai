{
  "name": "@tinyai/agent",
  "version": "0.0.2",
  "description": "A simple, light weight, and unopinionated ai framework.",
  "license": "ISC",
  "author": "",
  "homepage": "https://github.com/DaniloVlad/tinyai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniloVlad/tinyai.git"
  },
  "bugs": {
    "url": "https://github.com/DaniloVlad/tinyai/issues"
  },
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ai framework",
    "@tinyai/agent",
    "agent framework"
  ],
  "scripts": {
    "docs": "typedoc --out docs --theme default --tsconfig tsconfig.json --excludePrivate --excludeProtected --excludeExternals --includeVersion src/index.ts",
    "build": "rm -rf dist/ && tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "node --import tsx --watch -r ts-node/register src/index.ts",
    "prepublishOnly": "npm run build && npm run docs",
    "test": "jest",
    "test:tools": "jest --testPathPattern=tests/tools",
    "test:providers": "jest --testPathPattern=tests/providers",
    "test:utils": "jest --testPathPattern=tests/utils",
    "test:agents": "jest --testPathPattern=tests/agents",
    "test:mcp": "jest --testPathPattern=tests/mcp",
    "mock:mcp": "node --import tsx tests/mcp/mockMcpServer.ts"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/openai": "^1.3.17",
    "@ai-sdk/provider": "^1.1.3",
    "@ai-sdk/xai": "^1.2.15",
    "@modelcontextprotocol/sdk": "1.10.2",
    "ai": "^4.3.9",
    "zod": "3.25.1"
  }
}
